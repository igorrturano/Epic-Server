function DistanceCheck(who, mobontrack)
	var dist := MobDistance(who, mobontrack);
	if (dist == "perdeu")
		SendSysMessage(who, "Voce nao consegue mais identificar os rastros desse alvo", 3, 28);
		return 1;
	else
		return 0;
	endif
endfunction

