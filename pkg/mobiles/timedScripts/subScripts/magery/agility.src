 
use uo;
use os;

include ":attributes:attributes";

program TimedScript_Clumsy(params)
	var mobile := params[1];
	var attacker := params[2];
	var level := params[3];
	
	
	if ( GetObjProperty(mobile, "#AgilityMod") )
		// Condition is met if the mobile logs out and back in.
		// Prevents mod from being applied twice.
		return 0;
	endif


	SetObjProperty(mobile, "#AgilityMod", level);
	AP_ModifyStatMod(mobile, DEXTERITY, level);
	
	return 1;
endprogram
