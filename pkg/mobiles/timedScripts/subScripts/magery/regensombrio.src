use vitals;

program TimedScript_Clumsy(params)
	var mobile := params[1];
	var attacker := params[2];
	var level := params[3];

	
	var points := level;
	var i := 0;
		
	//if (GetObjProperty(mobile, "coMod") != error)
	//	return;
	//endif
	
	while(i < 10)
		HealFLS(attacker, points);
		i := i+1;
		sleep(2);
	endwhile
	
	return 1;
endprogram
